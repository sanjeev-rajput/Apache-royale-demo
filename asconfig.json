{
    "config": "royale",
    "compilerOptions": {
        "debug": false,
        "targets": ["JSRoyale"],
        "source-map": true,
        "js-library-path": [
            "../ruffle-schand-as-js-wasm/target",
            "libs"
        ],
        //"theme": "",
        "theme": "${royalelib}/themes/JewelTheme/src/main/resources/defaults.css",
        "source-path": [
            "src"

        ],
        "js-external-library-path": ["typedefs"],
        "remove-circulars": true,
        "html-template": "index-template.html",
        "js-compiler-option": [
            "--skip_type_inference"
        ]
    },
    "copySourcePathAssets": true,
    "additionalOptions": "-js-output-optimization=skipAsCoercions  -warn-public-vars=false -js-dynamic-access-unknown-members=true",
    "files":
    [
        "src/AsJsWasm.mxml"
    ]
    
  }